<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SalespersonDetailScreenController">
   <children>
      <Button layoutX="14.0" layoutY="460.0" mnemonicParsing="false" onAction="#toAdminHomeScreen" text="&lt;-- Back">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <ImageView fitHeight="188.0" fitWidth="204.0" layoutX="22.0" layoutY="88.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../main/salespeople.jpg" />
         </image>
      </ImageView>
      <Label fx:id="salespersonNameTextField" layoutX="40.0" layoutY="62.0" text="Last Name, First Name">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <TableView fx:id="appointmentsTable" layoutX="201.0" layoutY="111.0" prefHeight="169.0" prefWidth="378.0">
        <columns>
          <TableColumn fx:id="appointmentDateCol" prefWidth="75.0" text="Date" />
          <TableColumn fx:id="appointmentStartCol" prefWidth="89.20001220703125" text="Start" />
            <TableColumn fx:id="appointmentEndCol" prefWidth="84.5999755859375" text="End" />
            <TableColumn fx:id="appointmentCustomerNameCol" prefWidth="111.60000610351562" text="Customer" />
        </columns>
      </TableView>
      <Label layoutX="20.0" layoutY="293.0" text="Region:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label layoutX="348.0" layoutY="57.0" text="Appointments" underline="true">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <TableView fx:id="customersTable" layoutX="205.0" layoutY="317.0" prefHeight="169.0" prefWidth="378.0">
         <columns>
            <TableColumn fx:id="customerNameCol" prefWidth="99.80001831054688" text="Name" />
            <TableColumn fx:id="customerAddressCol" prefWidth="144.20001220703125" text="Address" />
            <TableColumn fx:id="totalCustomerAppointmentsCol" prefWidth="119.00003051757812" text="Total Appointments" />
         </columns>
      </TableView>
      <Label layoutX="360.0" layoutY="292.0" prefHeight="18.0" prefWidth="43.0" text="Clients" underline="true">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <RadioButton fx:id="upcomingAppointmentsRadioButton" layoutX="359.0" layoutY="90.0" mnemonicParsing="false" onAction="#onUpcomingAppointments" text="Upcoming">
         <toggleGroup>
            <ToggleGroup fx:id="appointmentsToggleGroup" />
         </toggleGroup>
         <font>
            <Font size="10.0" />
         </font></RadioButton>
      <RadioButton fx:id="allAppointmentsRadioButton" layoutX="272.0" layoutY="90.0" mnemonicParsing="false" onAction="#onAllAppointments" selected="true" text="All" toggleGroup="$appointmentsToggleGroup">
         <font>
            <Font size="10.0" />
         </font></RadioButton>
      <RadioButton fx:id="completedAppointmentsRadioButton" layoutX="469.0" layoutY="90.0" mnemonicParsing="false" onAction="#onCompletedAppointments" text="Completed" toggleGroup="$appointmentsToggleGroup">
         <font>
            <Font size="10.0" />
         </font></RadioButton>
      <Label fx:id="regionTextField" layoutX="145.0" layoutY="293.0" text="Label" />
      <Label layoutX="199.0" layoutY="5.0" text="Salesperson Detail" textFill="#0c56e8" underline="true">
         <font>
            <Font name="System Bold" size="24.0" />
         </font></Label>
      <Label layoutX="20.0" layoutY="343.0" text="Total Appointments:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="totalAppointmentsTextField" layoutX="145.0" layoutY="343.0" text="Label" />
      <Label layoutX="62.0" layoutY="368.0" text="Scheduled:" />
      <Label fx:id="scheduledAppointmentsTextField" layoutX="145.0" layoutY="368.0" text="Label" />
      <Label layoutX="61.0" layoutY="393.0" text="Completed:" />
      <Label fx:id="completedAppointmentsTextField" layoutX="145.0" layoutY="393.0" text="Label" />
      <Label layoutX="20.0" layoutY="318.0" text="Email:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="emailTextField" layoutX="145.0" layoutY="318.0" text="Label" />
      <Label layoutX="20.0" layoutY="418.0" text="Total Clients:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="totalCustomersTextField" layoutX="145.0" layoutY="418.0" text="Label" />
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#1f93ff00" height="397.0" layoutX="7.0" layoutY="52.0" stroke="BLACK" strokeType="INSIDE" width="187.0" />
      <Button layoutX="528.0" layoutY="5.0" mnemonicParsing="false" onAction="#toSignOut" text="Sign Out">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
